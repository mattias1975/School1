@model Student

    <div class="row" id="@("Student"+Model.Id)"></div>

<div class="col-4">@Model.Name</div>
<div class="col-3">@Model.CourseName</div>
<button class="btn btn-success btn-sm"
        onclick="Edit(@("Student"+Model.Id)','@Url.Action("Edit","Student",new { id = Model.Id })')")>

       </button>