@model  Student


<div class="row" id="@("Student" + Model.Id)" >

    <div class="col-3" style="background-color:antiquewhite">@Model.Name</div>

    <div class="col-3">@Model.Email</div>



    <div>
        <button class="btn btn-success btn-sm m-1"
                onclick="AddStudentToCourse('@("CourseStudent" + Model.Id)','@Model.Id','@Model.Id','@Url.Action("AddStudentToCourse","CourseStudent",new { id = Model.Id })')">
            Add Student to Course
        </button>


    </div>
</div>
